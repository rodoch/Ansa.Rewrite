<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.3</Version>
    <Authors>Ronan Doherty</Authors>
    <Company>Ronan Doherty</Company>
    <Copyright>Copyright 2019</Copyright>
    <Description>Prefabricated rules for common ASP.NET Core URL rewriting tasks</Description>
    <RepositoryType>Git</RepositoryType>
    <RepositoryUrl>https://github.com/rodoch/Ansa.Rewrite/</RepositoryUrl>
    <PackageId>Ansa.Rewrite</PackageId>
    <PackageTags>rewrite;rewriting;urlrewrite;aspnet;core</PackageTags>
    <PackageIconUrl></PackageIconUrl>
    <PackageProjectUrl>https://github.com/rodoch/Ansa.Rewrite/</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/rodoch/Ansa.Rewrite/blob/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>Initial prerelease trial</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Rewrite" Version="2.1.0" />
  </ItemGroup>

</Project>
